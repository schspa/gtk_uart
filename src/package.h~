#ifndef __PACKAGE_H__
#define __PACKAGE_H__

#define LINUX_HOST

#ifndef LINUX_HOST
#include "stm32f0xx.h"
#else
#include "type.h"
#endif //LINUX_HOST
struct package{
	uint8_t head[3];
	uint8_t channel;
	uint32_t value1;
	uint32_t value2;
	uint8_t checkout;
	uint8_t tail[4];
};

#endif //__PACKAGE_H__

